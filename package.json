{
  "name": "baggie",
  "version": "2.0.0",
  "private": true,
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "workspaces": [
    "packages/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:bag-of-tricks/baggie.git"
  },
  "keywords": [
    "javascript-library",
    "typescript-library",
    "library",
    "toolbox",
    "tools"
  ],
  "author": "Lars Munkholm <mail@larsersej.dk>",
  "license": "MIT",
  "homepage": "https://github.com/bag-of-tricks/baggie#readme",
  "scripts": {
    "#-- DEV": "exit",
    "storybook": "sb dev -p 6006",
    "": "exit",
    "#-- BUILD": "exit",
    "build": "yarn workspaces run build",
    "build:workspace": "node scripts/build-workspace.mjs",
    "build-storybook": "sb build",
    " ": "exit",
    "#-- QUALITY ASSURANCE": "exit",
    "prettier": "prettier --write \"./**/*.{js,ts,tsx,css,scss,json}\"",
    "lint": "npm-run-all lint:*",
    "lint:js": "tsc --noEmit && eslint packages/**/src/**/*.{ts,tsx,mdx} --fix --cache -c .eslintrc.commit.json --max-warnings=0",
    "lint:css": "stylelint \"packages/**/src/**/*.{,s}css\" --fix --cache --max-warnings=0",
    "test": "yarn workspaces run jest --passWithNoTests",
    "  ": "exit",
    "#-- AUTOMATION": "exit",
    "commit-msg": "node scripts/check-commit-msg",
    "postinstall": "syncpack fix-mismatches",
    "prepare": "rimraf .husky && husky uninstall && husky install .husky && npx husky add .husky/pre-commit \"npm run pre-commit\" && npx husky add .husky/commit-msg \"npm run commit-msg $1\"",
    "prepublish": "syncpack fix-mismatches && yarn run lint && yarn run prettier && yarn run test",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@storybook/addon-a11y": "^7.0.0-rc.7",
    "@storybook/addon-docs": "^7.0.0-rc.7",
    "@storybook/addon-essentials": "^7.0.0-rc.7",
    "@storybook/addon-links": "^7.0.0-rc.7",
    "@storybook/blocks": "^7.0.0-rc.7",
    "@storybook/cli": "^7.0.0-rc.7",
    "@storybook/client-api": "^7.0.0-rc.7",
    "@storybook/preview-web": "^7.0.0-rc.7",
    "@storybook/react": "^7.0.0-rc.7",
    "@storybook/react-vite": "^7.0.0-rc.7",
    "@types/jest": "^29.5.0",
    "@types/mdx": "^2.0.3",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-mdx": "^2.0.5",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-mdx": "^2.0.5",
    "eslint-plugin-prettier": "^5.0.0-alpha.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-unused-imports": "^2.0.0",
    "generate-template-files": "^3.2.1",
    "husky": "^8.0.3",
    "inquirer": "^9.1.5",
    "inquirer-autocomplete-prompt": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "^3.0.0-alpha.6",
    "remark-gfm": "^3.0.1",
    "rimraf": "^4.4.1",
    "sass": "^1.59.3",
    "stylelint": "^15.3.0",
    "stylelint-config-css-modules": "^4.2.0",
    "stylelint-config-standard-scss": "^7.0.1",
    "stylelint-order": "^6.0.3",
    "syncpack": "^9.8.4",
    "ts-jest": "^29.0.5",
    "typescript": "^5.0.2",
    "vite": "^4.2.1"
  }
}
