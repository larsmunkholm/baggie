{
  "name": "baggie",
  "version": "2.3.1",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:saxofonsolo/baggie.git"
  },
  "author": "Lars Munkholm <mail@larsersej.dk>",
  "license": "MIT",
  "homepage": "https://saxofonsolo.github.io/baggie",
  "keywords": [
    "javascript-library",
    "typescript-library",
    "library",
    "toolbox",
    "tools"
  ],
  "scripts": {
    "#-- DEV": "exit",
    "storybook": "pnpm sb dev -p 6006",
    "": "exit",
    "#-- BUILD": "exit",
    "build": "pnpm --recursive run build",
    "build:workspace": "node scripts/build-workspace.mjs",
    "build-storybook": "pnpm sb build --docs",
    " ": "exit",
    "#-- QUALITY ASSURANCE": "exit",
    "prettier": "prettier --write \"./**/*.{js,ts,tsx,css,scss,json}\"",
    "lint": "tsc --noEmit && eslint packages/**/src/**/*.{ts,tsx} --fix --cache -c .eslintrc.commit.json --max-warnings=0",
    "test": "pnpm --recursive run jest --passWithNoTests",
    "  ": "exit",
    "#-- AUTOMATION": "exit",
    "commit-msg": "node scripts/check-commit-msg.js",
    "prepare": "rimraf .husky && husky uninstall && husky install .husky && npx husky add .husky/pre-commit \"npm run pre-commit\" && npx husky add .husky/commit-msg \"npm run commit-msg \\$1\"",
    "prepublishOnly": "pnpm run lint && pnpm run prettier && pnpm run test",
    "pre-commit": "lint-staged"
  },
  "resolutions": {
    "strip-ansi": "^6.0.1"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1"
  },
  "devDependencies": {
    "@ngneat/falso": "^7.1.1",
    "@storybook/addon-a11y": "^7.6.3",
    "@storybook/addon-docs": "^7.6.3",
    "@storybook/addon-essentials": "^7.6.3",
    "@storybook/addon-links": "^7.6.3",
    "@storybook/blocks": "^7.6.3",
    "@storybook/cli": "^7.6.3",
    "@storybook/client-api": "^7.6.3",
    "@storybook/preview-web": "^7.6.3",
    "@storybook/react": "^7.6.3",
    "@storybook/react-vite": "^7.6.3",
    "@storybook/theming": "^7.6.3",
    "@types/jest": "^29.5.10",
    "@types/mdx": "^2.0.10",
    "@types/node": "^20.10.3",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-mdx": "^2.2.0",
    "eslint-plugin-css-modules": "^2.12.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-mdx": "^2.2.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "inquirer": "^9.2.12",
    "inquirer-autocomplete-prompt": "^3.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.32",
    "prettier": "3.1.0",
    "remark-gfm": "4.0.0",
    "rimraf": "^5.0.5",
    "sass": "^1.69.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2",
    "vite": "^5.0.5"
  }
}
