{
  "name": "baggie",
  "version": "2.2.2",
  "private": true,
  "workspaces": [
    "packages/core",
    "packages/react"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:saxofonsolo/baggie.git"
  },
  "author": "Lars Munkholm <mail@larsersej.dk>",
  "license": "MIT",
  "homepage": "https://saxofonsolo.github.io/baggie",
  "keywords": [
    "javascript-library",
    "typescript-library",
    "library",
    "toolbox",
    "tools"
  ],
  "scripts": {
    "#-- DEV": "exit",
    "storybook": "sb dev -p 6006",
    "": "exit",
    "#-- BUILD": "exit",
    "build": "yarn workspaces run build",
    "build:workspace": "node scripts/build-workspace.mjs",
    "build-storybook": "sb build --docs",
    " ": "exit",
    "#-- QUALITY ASSURANCE": "exit",
    "prettier": "prettier --write \"./**/*.{js,ts,tsx,css,scss,json}\"",
    "lint": "npm-run-all lint:*",
    "lint:js": "tsc --noEmit && eslint packages/**/src/**/*.{ts,tsx} --fix --cache -c .eslintrc.commit.json --max-warnings=0",
    "lint:css": "stylelint \"packages/**/src/**/*.{,s}css\" --fix --cache --max-warnings=0",
    "test": "yarn workspaces run jest --passWithNoTests",
    "  ": "exit",
    "#-- AUTOMATION": "exit",
    "commit-msg": "node scripts/check-commit-msg.js",
    "postinstall": "syncpack fix-mismatches",
    "prepare": "rimraf .husky && husky uninstall && husky install .husky && npx husky add .husky/pre-commit \"npm run pre-commit\" && npx husky add .husky/commit-msg \"npm run commit-msg \\$1\"",
    "prepublishOnly": "syncpack fix-mismatches && yarn run lint && yarn run prettier && yarn run test",
    "pre-commit": "lint-staged"
  },
  "resolutions": {
    "strip-ansi": "^6.0.1"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0"
  },
  "devDependencies": {
    "@ngneat/falso": "^7.1.0",
    "@storybook/addon-a11y": "^7.4.6",
    "@storybook/addon-docs": "^7.5.1",
    "@storybook/addon-essentials": "^7.4.6",
    "@storybook/addon-links": "^7.4.6",
    "@storybook/blocks": "^7.4.6",
    "@storybook/cli": "^7.4.6",
    "@storybook/client-api": "^7.4.6",
    "@storybook/preview-web": "^7.4.6",
    "@storybook/react": "^7.4.6",
    "@storybook/react-vite": "^7.4.6",
    "@types/jest": "^29.5.5",
    "@types/mdx": "^2.0.8",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "@vitejs/plugin-react-swc": "^3.4.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-mdx": "^2.2.0",
    "eslint-plugin-css-modules": "^2.11.4",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.4.2",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-mdx": "^2.2.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "inquirer": "^9.2.11",
    "inquirer-autocomplete-prompt": "^3.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^14.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "prettier": "3.0.3",
    "remark-gfm": "3.0.1",
    "rimraf": "^5.0.5",
    "sass": "^1.69.2",
    "stylelint": "^15.10.3",
    "stylelint-config-css-modules": "^4.3.0",
    "stylelint-config-standard-scss": "^11.0.0",
    "stylelint-order": "^6.0.3",
    "syncpack": "^11.2.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.11"
  }
}
