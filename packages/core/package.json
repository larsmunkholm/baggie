{
  "name": "@baggie/core",
  "version": "1.5.3",
  "description": "A library of helper functions for your TypeScript project.",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:saxofonsolo/baggie.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "rimraf ./lib && tsc -b ./tsconfig.build.cjs.json ./tsconfig.build.esm.json ./tsconfig.build.types.json",
    "test": "jest --verbose --runInBand",
    "deploy": "npm publish"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/types/index.d.ts",
  "files": [
    "lib",
    "src"
  ],
  "homepage": "https://saxofonsolo.github.io/baggie",
  "keywords": [
    "javascript-library",
    "typescript-library",
    "library",
    "toolbox",
    "tools"
  ],
  "sideEffects": false,
  "devDependencies": {
    "@baggie/react": "workspace:*",
    "jest": "^29.7.0",
    "rimraf": "^5.0.7",
    "typescript": "^5.4.5"
  },
  "gitHead": "985da4250f8f3aee48b8ec6ee5b810491fdf166d"
}
