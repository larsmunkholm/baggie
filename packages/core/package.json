{
  "name": "@baggie/core",
  "version": "1.5.0",
  "description": "A library of helper functions for your TypeScript project.",
  "license": "MIT",
  "scripts": {
    "build": "rimraf ./lib && bob build",
    "test": "jest --verbose --runInBand",
    "deploy": "npm publish"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/index.d.ts",
  "files": [
    "lib",
    "src"
  ],
  "homepage": "https://saxofonsolo.github.io/baggie",
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "keywords": [
    "javascript-library",
    "typescript-library",
    "library",
    "toolbox",
    "tools"
  ],
  "sideEffects": false,
  "devDependencies": {
    "jest": "^29.6.4",
    "rimraf": "^5.0.1",
    "typescript": "^5.2.2"
  },
  "gitHead": "985da4250f8f3aee48b8ec6ee5b810491fdf166d"
}
